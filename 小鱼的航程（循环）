import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner cin = new Scanner(System.in);
		
		int n = cin.nextInt();
		int m = cin.nextInt();
		int res = 0;
		for(int i = 1; i <= m; i ++ ) {
			if(n == 8) n = 1;
			if(n == 6 || n == 7) {
				n++;
				continue;
			}
			
			res += 250;
			n++;
		}
		System.out.println(res);
    }
}
