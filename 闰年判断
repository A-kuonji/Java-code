import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner cin = new Scanner(System.in);
		
		int year = cin.nextInt();
		
		if(year % 400 == 0 || year % 4 == 0 && (year % 100 != 0))
			System.out.println("yes");
		else System.out.println("no");
    }
}
