import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner cin = new Scanner(System.in);
		
		int n = cin.nextInt();		
		int m = cin.nextInt();
		
		int[][] a = new int[110][110];
		for(int i = 1; i <= n; i ++ ) {
			int ma = 0;
			for(int j = 1; j <= m; j ++ ) {
				a[i][j] = cin.nextInt();
				ma = a[i][j] > ma ? a[i][j] : ma;
			}
			System.out.println(ma);
		}
		
    }
}
